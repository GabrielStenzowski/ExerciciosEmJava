import java.util.Scanner;

public class program {
	public static void main(String[] args) {
		
		Scanner leitor = new Scanner(System.in);
		
		double repeticao = 5;
		
		double numero[] = new double[repeticao];
		
		double media = 0;
		
		for (int i = 0; i < numero.length; i++) {
			System.out.println("Escreva os numeros para o calculo da media: ");
			numero[i] = leitor.nextInt();			
			media = media + numero[i];			
		}
		
		media = media/repeticao;
		System.out.println("Sua media é: " + media);
		System.out.println("Numeros maiores ou iguais são: ");
		
		for (int i = 0; i < numero.length; i++) {
			if(numero[i] >= media) {
				System.out.println(numero[i]);
			}
			
		}
		
		leitor.close();
	
	}
	
	
}
